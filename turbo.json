{
  "$schema": "https://turborepo.org/schema.json",
  "tasks": {
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["!apps/storybook", "!apps/website"],
      "outputs": [".next/**", "dist/**"]
    },
    "clean": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "typecheck": {
      "outputs": []
    }
  }
}
