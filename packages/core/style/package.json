{
  "name": "@negative-space/style",
  "version": "1.1.3",
  "publishConfig": {
    "access": "public"
  },
  "description": "Core styling utility for NSUI components",
  "keywords": [
    "css",
    "inject",
    "nsui",
    "styles",
    "ui",
    "utility"
  ],
  "license": "MIT",
  "main": "src/index.ts",
  "module": "",
  "types": "",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/negative-space-ui/nsui.git",
    "directory": "packages/core/style"
  },
  "bugs": {
    "url": "https://github.com/negative-space-ui/nsui/issues"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist .turbo",
