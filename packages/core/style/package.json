    "lint": "eslint . --ext ts,tsx",
    "postpack": "clean-package restore",
    "prepack": "clean-package",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@negative-space/inject-css": "workspace:*",
    "@negative-space/types": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "clean-package": "../../../clean-package.config.json"
