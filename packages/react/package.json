{
  "name": "negative-space",
  "version": "2.3.0",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "description": "Negative Space core, providing flexible building blocks for custom and accessible UI development",
  "keywords": [
    "components",
    "design-system",
    "react",
    "negative-space",
    "nsui",
    "ui"
  ],
  "license": "MIT",
  "main": "src/index.ts",
  "module": "",
  "types": "",
  "files": [
    "dist"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/negative-space-ui/nsui.git.git",
    "directory": "packages/ui/button"
  },
  "bugs": {
    "url": "https://github.com/negative-space-ui/nsui/issues"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist .turbo",
    "dev": "pnpm build:fast --watch",
    "lint": "eslint . --ext ts,tsx",
    "postpack": "clean-package restore",
    "prepack": "clean-package",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@negative-space/action": "workspace:*",
    "@negative-space/data-display": "workspace:*",
    "@negative-space/data-entry": "workspace:*",
    "@negative-space/feedback": "workspace:*",
    "@negative-space/layout": "workspace:*",
    "@negative-space/nav": "workspace:*",
    "@negative-space/provider": "workspace:*",
    "@negative-space/typography": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  },
  "clean-package": "../../clean-package.config.json"
}
