{
  "name": "negative-space",
  "version": "2.4.9",
  "private": true,
  "publishConfig": {
    "access": "public"
  },
  "description": "Negative Space core, providing flexible building blocks for custom and accessible UI development",
  "keywords": [
    "components",
    "design-system",
    "react",
    "negative-space",
    "nsui",
    "ui"
  ],
  "license": "MIT",
  "main": "src/index.ts",
  "module": "",
  "types": "",
  "files": [
    "dist"
  ],
  "exports": {
    ".": "./src/index.ts",
    "./action": "./src/action/index.ts",
    "./data-display": "./src/data-display/index.ts",
    "./data-entry": "./src/data-entry/index.ts",
    "./feedback": "./src/feedback/index.ts",
    "./layout": "./src/layout/index.ts",
    "./nav": "./src/nav/index.ts",
    "./typography": "./src/typography/index.ts"
  },
  "sideEffects": false,
  "repository": {
    "type": "git",
    "url": "git+https://github.com/negative-space-ui/nsui.git.git",
    "directory": "packages/ui/button"
  },
  "bugs": {
    "url": "https://github.com/negative-space-ui/nsui/issues"
  },
  "scripts": {
    "build": "tsup",
    "clean": "rimraf dist .turbo",
    "dev": "pnpm build:fast --watch",
    "lint": "eslint . --ext ts,tsx",
    "postpack": "clean-package restore",
    "prepack": "pnpm build && node scripts/replace-workspace-versions.js && clean-package",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@negative-space/action": "workspace:*",
    "@negative-space/data-display": "workspace:*",
    "@negative-space/data-entry": "workspace:*",
    "@negative-space/feedback": "workspace:*",
    "@negative-space/layout": "workspace:*",
    "@negative-space/nav": "workspace:*",
    "@negative-space/provider": "workspace:*",
    "@negative-space/typography": "workspace:*"
  },
  "devDependencies": {
    "typescript": "^5.9.3"
  }
}
